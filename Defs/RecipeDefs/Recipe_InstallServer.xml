<Defs>
  <RecipeDef>
    <defName>Install_ServerBasic</defName>
    <label>install basic server</label>
    <description>Installing basic server</description>
    <jobString>Installing basic server.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>300</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ServerBasic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ServerBasic</li>
      </thingDefs>
    </fixedIngredientFilter>
    <skillRequirements>
      <Crafting>4</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <displayPriority>0</displayPriority>
	<workerClass>AdvancedCommercialServers.InstallServer_RecipeWorker</workerClass>
 </RecipeDef>
 
